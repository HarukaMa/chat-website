{
  "useTabs": false,
  "singleQuote": false,
  "trailingComma": "all",
  "semi": false,
  "printWidth": 140,
  "plugins": [
    "prettier-plugin-svelte"
  ],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte",
        "svelteSortOrder": "scripts-styles-markup-options",
        "svelteStrictMode": true,
			}
		}
	]
}
